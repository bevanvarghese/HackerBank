on className='voteButton' onClick={this.unlikeQuestion}>\r\n        <BsFillTriangleFill />\r\n        <span> Unvote</span>\r\n      </button>\r\n    ) : (\r\n      <button className='voteButton' onClick={this.likeQuestion}>\r\n        <BsTriangle />\r\n        <span> Upvote</span>\r\n      </button>\r\n    );\r\n\r\n    const questionCard = this.state.edit ? (\r\n      <div className='questionCard'>\r\n        <form>\r\n          Title\r\n          <br />\r\n          <input\r\n            type='text'\r\n            id='title'\r\n            name='title'\r\n            value={this.state.title}\r\n            onChange={this.handleChange}\r\n            required\r\n          />\r\n          <br />\r\n          <br />\r\n          Space\r\n          <br />\r\n          <input\r\n            type='radio'\r\n            name='space'\r\n            value='Algorithm'\r\n            defaultChecked={this.state.space == 'Algorithm'}\r\n            onClick={this.handleSelect}\r\n            required\r\n          />\r\n          Algorithm\r\n          <input\r\n            type='radio'\r\n            name='space'\r\n            value='Machine Learning'\r\n            defaultChecked={this.state.space == 'Machine Learning'}\r\n            onClick={this.handleSelect}\r\n            required\r\n          />\r\n          Machine Learning\r\n          <input\r\n            type='radio'\r\n            name='space'\r\n            value='System'\r\n            defaultChecked={this.state.space == 'System'}\r\n            onClick={this.handleSelect}\r\n            required\r\n          />\r\n          System\r\n          <input\r\n            type='radio'\r\n            name='space'\r\n            value='JavaScript'\r\n            defaultChecked={this.state.space == 'JavaScript'}\r\n            onClick={this.handleSelect}\r\n            required\r\n          />\r\n          JavaScript\r\n          <br />\r\n          <br />\r\n          Content\r\n          <br />\r\n          <textarea\r