      name='title'\r\n            value={this.state.title}\r\n            onChange={this.handleChange}\r\n            required\r\n          />\r\n          <br />\r\n          <br />\r\n          Space\r\n          <br />\r\n          <input\r\n            type='radio'\r\n            name='space'\r\n            value='Algorithm'\r\n            defaultChecked={this.state.space == 'Algorithm'}\r\n            onClick={this.handleSelect}\r\n            required\r\n          />\r\n          Algorithm\r\n          <input\r\n            type='radio'\r\n            name='space'\r\n            value='Machine Learning'\r\n            defaultChecked={this.state.space == 'Machine Learning'}\r\n            onClick={this.handleSelect}\r\n            required\r\n          />\r\n          Machine Learning\r\n          <input\r\n            type='radio'\r\n            name='space'\r\n            value='System'\r\n            defaultChecked={this.state.space == 'System'}\r\n            onClick={this.handleSelect}\r\n            required\r\n          />\r\n          System\r\n          <input\r\n            type='radio'\r\n            name='space'\r\n            value='JavaScript'\r\n            defaultChecked={this.state.space == 'JavaScript'}\r\n            onClick={this.handleSelect}\r\n            required\r\n          />\r\n          JavaScript\r\n          <br />\r\n          <br />\r\n          Content\r\n          <br />\r\n          <textarea\r\n            id='content'\r\n            name='content'\r\n            rows='4'\r\n            cols='50'\r\n            value={this.state.content}\r\n            onChange={this.handleChange}\r\n            required\r\n          />\r\n          <br />\r\n          <br />\r\n          <button className='editQuestionButton' onClick={this.handleSubmit}>\r\n            Submit\r\n          </button>\r\n        </form>\r\n        <p style={{ color: 'red' }}>{this.state.errors}</p>\r\n      </div>\r\n    ) : (\r\n      <div className='questionCard'>\r\n        <div className='questionHead'>\r\n          {voteButton}\r\n          <span> ({this.state.upvotes.length})</span>\r\n          {editAndDel}\r\n        </div>\r\n        <div className='questionMain'>\r\n          <span className='creator'>{this.state.askerName}</span>\r\n          <span className='createdOn'> posted on {this.state.time}</span>\r\n          <span className='questionSpace'>{this.state.space}</span>\r\n        </div>\r\n        <div className='questionBody'>\r\n          <p className='questionTitle'>{this.state.title}</p>\r\n          <p className='questionContent'>{this.state.content}</p>\r\n        </div>\r\n      </div>\r\n    );\r\n\r\n    const answerCards = this.state.answers.map((answer) => (\r\n      <Answer\r\n        key={answer.answerId}\r\n        creatorName={answer.creatorName}\r\n        content={answer.content}\r\n        time={\r\n          answer.time.substr(8, 2) +\r\n          '/' +\r\n          answer.time.substr(5, 2) +\r\n          '/' +\r\n          answer.time.substr(0, 4)\r\n        }\r\n        creatorId={answer.creatorId}\r\n        uid={this.state.uid}\r\n        aid={answer.aid}\r\n      />\r\n    ));\r\n\r\n    return (\r\n      <Fragment>\r\n        <NavBack />\r\n        <div style={{ width: '90%', marginLeft: 'auto', marginRight: 'auto' }}>\r\n          {questionCard}\r\n          {answerCards}\r\n          {this.state.uid != '' && (\r\n            <AnswerForm\r\n              uid={this.state.uid}\r\n              uname={this.state.uname}\r\n              qid={this.props.match.params.qid}\r\n            />\r\n          )}\r\n        </div>\r\n      </Fragment>\r\n    );\r\n  }\r\n}\r\n\r\nexport default Question;\r\n","D:\\Class\\Year 3 Sem 1\\COMP3322 - Modern Technologies on WWW\\Project\\HackerBank\\frontend\\src\\pages\\Main.js",["95","96","97","98","99","100","101","102","103","104"],"D:\\Class\\Year 3 Sem 1\\COMP3322 - Modern Technologies on WWW\\Project\\HackerBank\\frontend\\src\\components\\N